#!/usr/bin/env ruby
# frozen_string_literal: true

require "bundler/setup"
require "runtime-interfaces"

module Printable
  interface!

  require_method :name
  require_method :print, ->(a, b = 3, c:, d: 1, &block) { }
end

class User
  implements Printable

  def name
  end

  def print(&block)

  end
  verify_interfaces!
end

